{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "lolytics-adf"
		},
		"AzureBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlob'"
		},
		"riot_rest_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'riot_rest'"
		},
		"riot_rest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://americas.api.riotgames.com/lol/match/v5/matches/by-puuid/"
		},
		"riot_rest_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "n202u000107"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "lolytics-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlob_connectionString": {
						"value": "[parameters('AzureBlob_connectionString')]"
					},
					"riot_rest_password": {
						"value": "[parameters('riot_rest_password')]"
					},
					"riot_rest_properties_typeProperties_url": {
						"value": "[parameters('riot_rest_properties_typeProperties_url')]"
					},
					"riot_rest_properties_typeProperties_userName": {
						"value": "[parameters('riot_rest_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}